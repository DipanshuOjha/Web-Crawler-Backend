{
  "version": 2,
  "builds": [
    {
      "src": "gui/gui.go",
      "use": "@vercel/go",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/gui/gui.go"
    },
    {
      "src": "/(.*)", 
      "dest": "/gui/gui.go"
    }
  ]
}